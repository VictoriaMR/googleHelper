{
	"manifest_version": 3,
	"name": "产品数据助手",
	"version": "3.0",
	"description": "抓取页面产品信息, 同时修改整理产品",
	"action": {
		"default_icon": "icon.png",
		"default_title": "数据助手",
		"default_popup": "menu.html"
	},
	"icons" : {
		"24": "icon.png",
		"48": "icon.png"
	},
	"background": {
		// "service_worker": ["jquery.js","background.js","socket.io.js","socket.js"]
		"service_worker": "background.js"
	},
	"content_scripts": [{
		"matches": [
			"*://*.1688.com/*",
			"*://*.tmall.com/*",
			"*://*.taobao.com/*"
		],
		"js": [
			"content.js"
		]
	}],
	"permissions": [
		"storage",
		"activeTab",
		"cookies",
		"webRequest"
	],
	"host_permissions": ["http://*/*", "https://*/*"]
}