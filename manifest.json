{
	"manifest_version": 2,
	"name": "产品数据助手",
	"version": "2.0",
	"description": "供应商产品数据助手",
	"browser_action": {
		"default_popup": "popup/popup.html",
		"default_icon": {
			"16": "images/icon-16.png",
			"32": "images/icon-32.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		}
	},
	"icons" : {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"background": {
		"scripts": ["scripts/background.js"]
	},
	"content_scripts": [{
		"js": ["scripts/content.js"],
		"matches": [
			"*://*.1688.com/*",
			"*://*.tmall.com/*",
			"*://*.taobao.com/*"
		],
		"all_frames": false
	}],
	"permissions": [
		"tabs",
	    "cookies",
	    "webRequest"
	]
}